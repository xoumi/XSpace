{
  "name": "homenext",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "updateMeta": "node src/util/metaUpdater.mjs"
  },
  "dependencies": {
    "execa": "^6.1.0",
    "gray-matter": "^4.0.3",
    "gsap": "^3.8.0",
    "hastscript": "^7.0.2",
    "lodash-es": "^4.17.21",
    "mdx-bundler": "7.0.0",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remark": "^14.0.2",
    "remark-prism": "^1.3.6",
    "remark-stringify": "^10.0.2",
    "simplebar-react": "^2.3.7",
    "staged-git-files": "^1.3.0",
    "to-vfile": "^7.2.3",
    "unist-util-find-after": "^4.0.0",
    "unist-util-remove": "^3.1.0",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@next/eslint-plugin-next": "^12.1.6",
    "@types/lodash-es": "^4.17.5",
    "@types/mdast": "^3.0.10",
    "@types/react": "17.0.19",
    "@types/remark-prism": "^1.3.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "esbuild": "^0.13.4",
    "eslint": "7.32",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "^12.1.6",
    "eslint-plugin-react": "^7.29.4",
    "estree-util-value-to-estree": "^1.3.0",
    "mdast-util-to-string": "^3.1.0",
    "postcss": "^8.3.8",
    "remark-directive": "^2.0.0",
    "sass": "^1.39.0",
    "typescript": "4.4.2",
    "unified": "^10.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json",
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "@next/eslint-plugin-next"
    ],
    "extends": [
      "eslint:recommended",
      "airbnb-base",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "plugin:@next/next/recommended",
      "next",
      "next/core-web-vitals"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": 0,
      "import/extensions": 0,
      "quotes": [
        1,
        "single"
      ],
      "max-len": 0,
      "arrow-spacing": [
        1,
        {
          "before": true,
          "after": true
        }
      ],
      "no-use-before-define": 0,
      "@typescript-eslint/no-use-before-define": [
        1
      ]
    }
  },
  "eslintIgnore": [
    "**/node_modules/*",
    "dist",
    "coverage",
    "**/.next/*",
    ".sass",
    "*.mjs",
    "next-env.d.ts"
  ]
}
